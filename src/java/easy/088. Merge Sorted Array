class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        if((m+n)>m){
            for(int i = 0;i<n;i++){
                nums1[m+i] = nums2[i];
            }
            int index = 0;
            for(int j = 0;j<n;j++){
                for(int i = 0;i<m+j;i++){
                    if(nums1[i]>= nums1[m+j]){
                        int temp = nums1[i];
                        nums1[i] = nums1[m+j];
                        nums1[m+j] = temp;
                    }
                }        
            }
            
        }
    }
}
